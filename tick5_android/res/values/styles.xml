<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="Theme.Sherlock.Light.DarkActionBar">
        <item name="actionBarStyle">@style/AppTheme.ActionBarStyle</item>
    </style>

    <style name="AppTheme.ActionBarStyle" parent="Widget.Sherlock.Light.ActionBar.Solid.Inverse"></style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="AppTheme.HoloSpinner" parent="@style/Widget.Sherlock.Light.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_holo</item>
    </style>

    <style name="Tick5Widget">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorLink">@android:color/white</item>
    </style>

    <style name="Tick5Widget.Text">
        <item name="android:background">@drawable/text_background</item>
        <item name="android:autoLink">web</item>
        <item name="android:linksClickable">true</item>
    </style>

</resources>